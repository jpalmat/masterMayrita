Main_Accelerator ACCELERATORS 
BEGIN
	"^L", IDM_FILE_LOAD
	"^S", IDM_FILE_SAVE
	"^E", IDM_FILE_EDITOR
	"^Q", IDM_FILE_QUIT
	"^R", IDM_EXE_RUN
	"^T", IDM_EXE_STEP
	"^W", IDM_EXE_WATCH
	"^C", IDM_EXE_HALT
	"^C", IDM_EXE_HALT_NOW, VIRTKEY, SHIFT
	"^J", IDM_HELP_COMPLETE, ASCII, NOINVERT
	"^U", IDM_EXE_RESET
	"^V", IDM_EDIT_PASTE
/*        VK_CANCEL, IDM_EXE_HALT, VIRTKEY, CONTROL  */
END

